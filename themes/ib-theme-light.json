        "foreground": "#161616"
      }
    },
    {
      "scope": "variable.language",
      "settings": {
        "foreground": "#da1e28"
      }
    },
    {
      "scope": "support.function",
      "settings": {
        "foreground": "#0043ce"
      }
    },
    {
      "scope": "support.class",
      "settings": {
        "foreground": "#9f1853"
      }
    },
    {
      "scope": "support.type",
      "settings": {
        "foreground": "#9f1853"
      }
    },
    {
