  "tokenColors": [
    {
      "scope": "comment",
      "settings": {
        "foreground": "#6f6f6f",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "string",
      "settings": {
        "foreground": "#198038"
      }
    },
    {
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#da1e28"
      }
    },
    {
      "scope": "constant.language",
      "settings": {
        "foreground": "#da1e28"
      }
    },
    {
      "scope": "keyword",
      "settings": {
        "foreground": "#8a3ffc",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "storage",
      "settings": {
        "foreground": "#8a3ffc",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "entity.name.function",
      "settings": {
        "foreground": "#0043ce"
      }
    },
    {
      "scope": "entity.name.class",
      "settings": {
        "foreground": "#9f1853"
      }
    },
    {
      "scope": "entity.name.type",
      "settings": {
        "foreground": "#9f1853"
      }
    },
    {
      "scope": "variable.parameter",
